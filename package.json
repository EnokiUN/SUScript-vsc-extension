{
    "name": "sus",
    "displayName": "StoryUtilityScript",
    "description": "The Visual Studio Code extension for the SUScript language.",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.62.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "sus",
            "aliases": ["SUS", "sus"],
            "extensions": [".sus"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "sus",
            "scopeName": "source.sus",
            "path": "./syntaxes/sus.tmLanguage.json"
        }]
    }
}
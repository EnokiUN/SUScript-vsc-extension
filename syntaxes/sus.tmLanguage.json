{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "SUS",
	"patterns": [
		{
			"include": "#keywords"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [{
				"name": "keyword.control.sus",
				"match": "\\b(if|while|for|return)\\b"
			}]
		},
		"comments": {
			"patterns": [{
				"name": "comment.line.sus",
				"match": "# .*"
			}]
		}
	},
	"scopeName": "source.sus"
}